;; Things added by Custom. There should be only one instance of `custom-*`. If
;; there is more than one, they won't work right.
(custom-set-variables
 )

(custom-set-faces
 (if (eq system-type 'darwin)
     '(default ((t (:height 85 :foundry "apple" :family "DejaVu LGC Sans Mono")))))
 '(js2-function-param ((t (:foreground "burlywood"))) t))
