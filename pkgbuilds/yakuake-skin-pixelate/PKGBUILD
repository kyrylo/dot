pkgname=yakuake-skin-pixelate
pkgver=0.2.1
pkgrel=1
pkgdesc="Yakuake skin with some orange insertions."
arch=('i686' 'x86_64')
url="http://kde-look.org/content/show.php/Pixelate?content=139472"
license=('MIT License')
depends=('yakuake')
source=(http://kde-look.org/CONTENT/content-files/139472-pixelate.tar.gz)
build() {

  mkdir -p ${pkgdir}/usr/share/apps/yakuake/skins/
  cp -r ${srcdir}/pixelate ${pkgdir}/usr/share/apps/yakuake/skins/

}
md5sums=('b3b6f84e30853212ac03849099939776')

